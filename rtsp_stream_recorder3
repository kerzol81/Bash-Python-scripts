#!/usr/bin/python

class Vivotek():
    ip = '192.168.1.10'
    username = 'root'
    password = 'root'
    stream = '3'
    port = '554'

    def url(self):
        return ('rtsp://%s:%s@%s:%s/live%s.sdp') % (self.username, self.password, self.ip, self.port, self.stream)
        
class Axis():
    ip = '192.168.1.90'
    username = 'admin'
    password = 'admin'
    
    def url(self):
        return ('rtsp://%s:%s@%s/axis-media/media.amp') % (self.username, self.password, self.ip)



